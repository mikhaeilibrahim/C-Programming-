#include <stdio.h>
#include <stdlib.h>

int main()
{

    unsigned int x,y,i;

    while(1)
    {
        printf("Enter 2 numbers\n");
        scanf("%d%d",&x,&y);

        if(x>=y)
        {
            for(i=x-1; i>y; i--)
            {
                printf("%d  ",i);
            }
        }
        else
        {
            for(i=y-1; i>x; i--)
            {
                printf("%d  ",i);
            }
        }

        printf("\n");
    }

return 0;
}
