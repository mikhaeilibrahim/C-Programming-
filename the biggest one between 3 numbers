#include <stdio.h>
#include <stdlib.h>

int main()
{

    unsigned int x,y,z;
    printf("Enter 3 numbers to know the biggest one\n");

    while(1)
    {
        scanf("%d%d%d",&x,&y,&z);

        if((x>=y)&&(x>=z))
        {
            printf("%d",x);
        }
        else if ((y>=x)&&(y>=z))
        {
            printf("%d",y);
        }
        else
        {
            printf("%d",z);
        }

        printf("\n");

    }


    return 0;
}
