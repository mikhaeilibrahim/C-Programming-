#include <stdio.h>
#include <stdlib.h>

int main()
{

    signed int x,i;

    while(1)
    {
        scanf("%d",&x);

        if (x==0)
        {
            break;
        }

        for(i=0; i<x; i++)
        {
            if(i%2==0)
            {
                printf("%d  ",i);
            }
        }
        printf("\n");
    }
    return 0;
}
