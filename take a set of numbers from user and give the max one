#include <stdio.h>
#include <stdlib.h>

int main()
{

    signed int n,x,max,i;

    while(1)
    {
        printf("Enter number of numbers you want to check\n");
        scanf("%d",&n);
        max=-4000000000;

        for(i=0;i<n;i++)
        {
            scanf("%d",&x);

            if(i==0)
            {
                max=x;
            }

            if(x>max)
            {
                max=x;
            }
        }
        printf("max=%d\n",max);

    }

return 0;
}
