#include <stdio.h>
#include <stdlib.h>

int main()
{

    unsigned int x;

    while(1)
    {
        printf("Enter your grade\n");
        scanf("%d",&x);

        if(x>=0 && x<50)
        {
            printf("F\n");
        }
        else if (x>=50 && x<60)
        {
            printf("D\n");
        }
        else if (x>=60 && x<70)
        {
            printf("C\n");
        }
        else if (x>=70 && x<80)
        {
            printf("B\n");
        }
        else if (x>=80 && x<=100)
        {
            printf("A\n");
        }
        else
        {
            printf("Error\n");
        }

    }
    return 0;
}
