#include <stdio.h>
#include <stdlib.h>

int main()
{

    signed int x,y,i,sum,temp;

    while(1)
    {
        sum=0;
        printf("Enter 2 numbers\n");
        scanf("%d%d",&x,&y);

        if(y>x)
        {
            temp=x;
            x=y;
            y=temp;
        }

        for(i=x-1; i>y; i--)
        {
            sum=sum+i;
        }
        printf("Sum=%d\n",sum);


    }

    return 0;
}
