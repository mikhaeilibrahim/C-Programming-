#include <stdio.h>
#include <stdlib.h>

int main()
{
  unsigned int n;

  while(1)
  {
      fflush(stdin);
      scanf("%c",&n);

      if(n>='A' && n<='Z')
      {
          printf("CAPITAL\n");
      }
      else if (n>='a' && n<='z')
      {
          printf("small\n");
      }
      else if (n>='0' && n<='9')
      {
          printf("Number\n");
      }
      else
      {
          printf("symbol\n");
      }

  }
    return 0;
}
