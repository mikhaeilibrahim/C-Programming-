#include <stdio.h>
#include <stdlib.h>

int main()
{

    unsigned int x,y,i,result;

    while(1)
    {
        result=1;
       printf("Enter base\n");
       scanf("%d",&x);
       printf("Enter power\n");
       scanf("%d",&y);

       for(i=y;i>0;i--)
       {
           result=result*x;
       }
       printf("Result=%d\n",result);
    }

return 0;
}
