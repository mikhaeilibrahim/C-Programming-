#include <stdio.h>
#include <stdlib.h>

int main()
{

    unsigned int x,y,i,swap;

    while(1)
    {
        printf("Enter 2 numbers\n");
        scanf("%d%d",&x,&y);

        if(y>x)
        {
            swap=x;
            x=y;
            y=swap;
        }

        for(i=x-1;i>y;i--)
        {
            printf("%d  ",i);
        }

        printf("\n");
    }

return 0;
}
