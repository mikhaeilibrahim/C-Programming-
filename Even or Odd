#include <stdio.h>
#include <stdlib.h>

int main()
{

    unsigned int x;
    printf("Enter number to check even or odd\n");

    while(1)
    {
        scanf("%d",&x);
        if(x%2==0)
        {
            printf("Even\n");
        }
        else
        {
            printf("Odd\n");
        }

    }


    return 0;
}
